(* type: work_gga_x *)

c     :=  0.7168:
alpha :=  2.804:
d     := 28.23705740248932030511071641312341561894: (* POW(CBRT(4.0/3.0) * 2.0*M_PI/3.0, 4) *)

csi  := s -> (3.0/2.0 * LambertW(s^(3.0/2.0) / (2.0*sqrt(6.0))))^(2.0/3.0):
fb   := s -> Pi/3.0 * s/(csi(s) * (d + csi(s)^2)^(1.0/4.0)):
flaa := s -> (1.0 + c*s^2)/(1.0 + c*s^2/fb(s)):
XX   := s -> 1.0 - alpha*s^2/(1.0 + alpha*s^2):

f    := x ->  XX(X2S*x) + (1.0 - XX(X2S*x))*flaa(X2S*x):
